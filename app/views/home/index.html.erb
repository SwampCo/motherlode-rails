<div class="jumbotron">
  <div class="container">
    <div class="col-md-4">
      <h1>Explore</h1>
      <p>&nbsp;</p>
      <p class="cols-md-5">
        <%= select "product", "id", @products.map {|u| [u.name, u.id]}, {:include_blank => 'Select a product...'}, class: "form-control" %>
      </p>
    </div>
    <div class="col-md-4 col-md-offset-2">
      <h1>Contribute</h1>
      <p>&nbsp;</p>
      <p class="center-block">
        <a class="btn btn-info btn-lg" role="button">Learn more &raquo;</a>
        <a class="btn btn-primary btn-lg" role="button">Signup!</a>
      </p>
    </div>
  </div>
</div>